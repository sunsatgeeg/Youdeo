<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itwill.shop.user.mapper.MemberMapper">
	<insert id="create" parameterType="com.geeg.youdeo.member.Member">
		insert into member(m_id,m_password,m_name,m_email,m_birthday,m_phone) 
		values(#{m_id},#{m_password},#{m_name},#{m_email},#{m_birthday},#{m_phone})
	</insert>
	
	<select id="findMember" parameterType="java.lang.String"
		resultType="com.geeg.youdeo.member.Member">
		select * from member
		where m_id=#{m_id}
	</select>
</mapper>